@model UniversityContracts.BindingModels.ReportBindingModel

@{ ViewData["Title"] = "Educations Report"; }

<h1>Получить список</h1>
<hr />

<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="EducationIds" class="control-label"></label>
                <select asp-for="EducationIds" id="discipline" class="form-control" mu asp-items="ViewBag.EducationIds"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Получить doc" asp-action="ReportDoc" class="btn btn-primary" />
            </div>
            <div class="form-group">
                <input type="submit" value="Получить xls" asp-action="ReportXls" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}